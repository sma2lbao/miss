    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.32",
    "apollo-upload-client": "^13.0.0",
    "draft-js": "^0.11.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "firebase": "^7.16.1",
    "graphql": "^14.7.0",
    "hls.js": "^0.12.4",
    "husky": "^3.1.0",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^9.4.3",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "notistack": "^0.9.6",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-player": "^1.14.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-swipeable-views": "^0.13.3",
    "react-test-renderer": "^16.13.1",
    "subscriptions-transport-ws": "^0.9.17",
    "three": "^0.112.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "generate": "graphql-codegen --config codegen.yml",
    "postbuild": "react-snap"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "additional-rule": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "additional-typescript-only-rule": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.0",
    "@graphql-codegen/fragment-matcher": "1.17.0",
    "@graphql-codegen/introspection": "1.17.0",
    "@graphql-codegen/typescript": "1.17.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.0",
    "@graphql-codegen/typescript-graphql-files-modules": "1.17.0",
    "@graphql-codegen/typescript-operations": "1.17.0",
    "@graphql-codegen/typescript-react-apollo": "1.17.0",
    "@sentry/browser": "^5.19.2",
    "@types/apollo-upload-client": "^8.1.3",
    "@types/draft-js": "^0.10.43",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/hls.js": "^0.12.5",
    "@types/jest": "24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/node": "12.12.11",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/react-test-renderer": "^16.9.2",
    "apollo": "^2.30.1",
    "react-app-rewired": "^2.1.5",
    "react-snap": "^1.23.0"
  },
  "reactSnap": {
    "inlineCss": true,
    "puppeteerIgnoreHTTPSErrors": true
  }
}
